{:paths ["src"
         "test"
         "bench"
         "resources"]
 :deps
 {teknql/systemic                    {:local/root "../../teknql/systemic"}
  teknql/wing                        {:local/root "../../teknql/wing"}
  aero/aero                          {:mvn/version "1.1.6"}
  babashka/babashka                  {:mvn/version "0.8.157"}
  babashka/babashka.pods             {:mvn/version "0.1.0"}
  babashka/babashka.curl             {:mvn/version "0.1.2"}
  babashka/process                   {:mvn/version "0.1.4"}
  babashka/fs                        {:mvn/version "0.1.6"}
  metosin/malli                      {:mvn/version "0.8.9"}
  com.lambdaisland/specmonstah-malli {:mvn/version "0.0.7"}
