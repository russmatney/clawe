  org.clojure/tools.reader           {:mvn/version "1.3.6"}
  backtick/backtick                  {:mvn/version "0.3.4"}
  aysylu/loom                        {:mvn/version "1.0.2"}

  russmatney/org-crud {:local/root "../org-crud"}}
 :aliases
 {:dev      {:jvm-opts ["-XX:-OmitStackTraceInFastThrow"]}
  :carve    {:extra-deps {borkdude/carve
                          {:git/url "https://github.com/borkdude/carve"
                           :sha     "df552797a198b6701fb2d92390fce7c59205ea77"}}
             :jvm-opts   ["-XX:-OmitStackTraceInFastThrow"]
             :main-opts  ["-m" "carve.main"]}
  :outdated {:extra-deps {com.github.liquidz/antq {:mvn/version "1.8.847"}}
             :main-opts  ["-m" "antq.core"]}

  ;; exclude these from uberjar builds
  ;; :remove-clojure
